                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"\[ImaginaryI]", "/", "2"}], "*", 
                   RowBox[{"w", ".", "\[Sigma]"}]}], ")"}], ".", 
                 RowBox[{"doubletFields", "[", 
                  RowBox[{"[", "i", "]"}], "]"}]}]}], ")"}], "]"}]}], "]"}], 
           ",", 
           RowBox[{"Assumptions", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{
