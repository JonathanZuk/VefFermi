Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"massMatrix", "::", "usage"}], "=", 
   "\"\<massMatrix[potential,singVacua,doubVacua,tripVacua] calculates \
(really the inverse propagator) of a theory in Fermi gauge given the scalar \
potential written with singlets as s#, doublet d# and triplets \
\[CapitalSigma]#; and the associated background fields\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"masses", "::", "usage"}], "=", 
   "\"\<masses[massMatrix, parameters] calculates the mass spectrum of the \
scalar and gauge components of the theory in Fermi gauge (given as squared \
masses)\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"vTree", "::", "usage"}], " ", "=", " ", 
   "\"\<vTreepotential_,singVacua,doubVacua,tripVacua] calculates the \
tree-level effective potential for a theory given the scalar potential \
written with singlets as s#, doublet d# and triplets \[CapitalSigma]#; and \
the associated background fields\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"vColemanWeinberg", "::", "usage"}], "=", 
   "\"\<vColemanWeinberg[potential,singVacua,doubVacua,tripVacua,\
fermionMasses,parameters] calculates the 1-loop correction to the effective \
potential of a theory in Fermi gauge and MSbar scheme given the scalar \
potential written with singlets as s#, doublet d# and triplets \
\[CapitalSigma]#; the associated background fields; and the fermion \
masses\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"vThermal", "::", "usage"}], "=", 
   "\"\<vThermal[potential,singVacua,doubVacua,tripVacua,fermionMasses,\
temperature,parameters] gives the 1-loop thermal correction to the effective \
potential of a theory in Fermi gauge and MSbar scheme given the scalar \
potential written with singlets as s#, doublet d# and triplets \
\[CapitalSigma]#; the associated background fields; and the fermion \
masses\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"JB", "::", "usage"}], "=", 
  "\"\<JB[\!\(\*SuperscriptBox[\(x\), \(2\)]\)] evaluates the thermal bosonic \
function of x^2\>\""}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"JB", "::", "usage"}], "=", 
  "\"\<JF[\!\(\*SuperscriptBox[\(x\), \(2\)]\)] evaluates the thermal \
