Cell[BoxData[
 RowBox[{
  RowBox[{"massMatrix", "[", 
   RowBox[{
   "potential_", ",", "singVacua_", ",", "doubVacua_", ",", "tripVacua_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "fields", ",", "tempDoubFields", ",", "doubletFields", ",", "doublets", 
      ",", "doubRules", ",", "tempSingFields", ",", "singletFields", ",", 
      "singlets", ",", "singRules", ",", "tempTripFields", ",", 
      "tripletFields", ",", "triplets", ",", "tripRules", ",", 
      "potentialTerms", ",", "doubletKineticTerms", ",", 
      "singletKineticTerms", ",", "tripletKineticTerms", ",", "gaugeTerms", 
      ",", "out", ",", "\[Sigma]", ",", "w", ",", "t"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "preliminary", " ", "setup", " ", "of", " ", "generators", " ", "and", 
      " ", "vector", " ", "of", " ", "gauge", " ", "terms"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Sigma]", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "0"}], "}"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", 
            RowBox[{"-", "\[ImaginaryI]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[ImaginaryI]", ",", "0"}], "}"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"1", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", 
            RowBox[{"-", "1"}]}], "}"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"1", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}]}], "}"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"g2", "*", "w1"}], ",", 
        RowBox[{"g2", "*", "w2"}], ",", 
        RowBox[{"g2", "*", "w3"}], ",", 
        RowBox[{"g1", "*", "b"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"t", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"Sqrt", "[", "2", "]"}]}], "*", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "1", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "0", ",", "1"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "1", ",", "0"}], "}"}]}], "}"}]}], ",", 
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"Sqrt", "[", "2", "]"}]}], "*", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"-", "\[ImaginaryI]"}], ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"\[ImaginaryI]", ",", "0", ",", 
             RowBox[{"-", "\[ImaginaryI]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "\[ImaginaryI]", ",", "0"}], "}"}]}], "}"}]}], 
        ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"1", ",", "0", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", 
            RowBox[{"-", "1"}]}], "}"}]}], "}"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Create", " ", "symbolic", " ", "fields", " ", "decomposed", " ", 
       "into", " ", "individual", " ", "scalars", " ", "from", " ", 
       "Lagrangian", " ", "terms", " ", "using", " ", "vacuum", " ", 
       "configurations", " ", "to", " ", "identify", " ", "number", " ", "of",
        " ", "fields"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"tempDoubFields", "=", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["h", "##"], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Length", "[", "doubVacua", "]"}], ",", "4"}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"doubletFields", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"tempDoubFields", ".", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"1", ",", "0"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "1"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"\[ImaginaryI]", ",", "0"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "\[ImaginaryI]"}], "}"}]}], "}"}]}], ")"}], 
        "/", 
        RowBox[{"Sqrt", "[", "2", "]"}]}], "+", "doubVacua"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"doublets", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Symbol", "[", 
         RowBox[{"\"\<h\>\"", "<>", 
          RowBox[{"ToString", "[", "i", "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "doubVacua", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"doubRules", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"#1", "->", "#2"}], ")"}], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"doublets", ",", "doubletFields"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tempSingFields", "=", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["s", "##"], "&"}], ",", 
        RowBox[{"Length", "[", "singVacua", "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"singletFields", "=", 
      RowBox[{"tempSingFields", "+", "singVacua"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"singlets", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Symbol", "[", 
         RowBox[{"\"\<s\>\"", "<>", 
          RowBox[{"ToString", "[", "i", "]"}]}], "]"}], ",", 
