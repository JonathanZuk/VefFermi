     RowBox[{"singletKineticTerms", "=", 
      RowBox[{
       RowBox[{"p", "^", "2"}], "*", 
       RowBox[{
        RowBox[{"tempSingFields", ".", "tempSingFields"}], "/", "2"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"tripletKineticTerms", "=", 
      RowBox[{"Tr", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Simplify", "[", 
          RowBox[{
           RowBox[{"ComplexExpand", "[", 
