     "\[IndentingNewLine]", 
     RowBox[{"tempTripFields", "=", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[CapitalSigma]", "##"], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Length", "[", "tripVacua", "]"}], ",", "3"}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tripletFields", "=", 
      RowBox[{"tempTripFields", "+", "tripVacua"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"triplets", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Symbol", "[", 
         RowBox[{"\"\<\[CapitalSigma]\>\"", "<>", 
          RowBox[{"ToString", "[", "i", "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "tripVacua", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tripRules", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"#1", "->", "#2"}], ")"}], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"triplets", ",", "tripletFields"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Assemble", " ", "vector", " ", "of", " ", "scalar", " ", "fields"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"fields", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Join", "[", 
        RowBox[{
        "tempDoubFields", ",", "tempSingFields", ",", "tempTripFields"}], 
