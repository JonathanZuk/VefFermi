               RowBox[{
                RowBox[{"(", 
                 RowBox[{"\[ImaginaryI]", "*", 
                  RowBox[{
                   RowBox[{"w", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", "3"}], "]"}], "]"}], ".", "t"}]}], ")"}], 
                ".", 
                RowBox[{"tripletFields", "[", 
                 RowBox[{"[", "i", "]"}], "]"}]}]}], ")"}], ".", 
             RowBox[{"Conjugate", "[", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"p", "*", 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"tripletFields", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "-", 
                   RowBox[{"tripVacua", "[", 
                    RowBox[{"[", "i", "]"}], "]"}]}], ")"}]}], "-", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"\[ImaginaryI]", "*", 
                   RowBox[{
                    RowBox[{"w", "[", 
                    RowBox[{"[", 
                    RowBox[{";;", "3"}], "]"}], "]"}], ".", "t"}]}], ")"}], 
                 ".", 
                 RowBox[{"tripletFields", "[", 
                  RowBox[{"[", "i", "]"}], "]"}]}]}], ")"}], "]"}]}], "]"}], 
           ",", 
           RowBox[{"Assumptions", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"fields", "\[Element]", "Reals"}], ",", 
              RowBox[{"_Symbol", "\[Element]", "Reals"}]}], "}"}]}]}], "]"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"Length", "[", "tripVacua", "]"}]}], "}"}]}], "]"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
